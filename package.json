{
	"name": "yamlnav",
	"displayName": "YAMLNav",
	"description": "Navigate through YAML keys",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:yamlnav.search",
		"onCommand:yamlnav.copy"
	],
	"main": "./extension.js",
	"publisher": "aubinlrx",
	"repository": "https://github.com/aubinlrx/vscode-yamlnav",
	"contributes": {
		"commands": [
			{
				"command": "yamlnav.search",
				"title": "YAMLNav: Search"
			},
			{
				"command": "yamlnav.copy",
				"title": "YAMLNav: Copy"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
